     ac_rfnd_pymt."source_no" AS ac_rfnd_pymt_source_no,
     ac_knof."fd_st" AS ac_knof_fd_st,
     ac_advc_pymt."receipt_id" AS ac_advc_pymt_receipt_id,
     ac_rcpt."id" AS ac_rcpt_id,
     ac_rcpt."audit_no" AS ac_rcpt_audit_no,
     ac_rcpt."cancel_comment" AS ac_rcpt_cancel_comment,
     ac_rcpt."description" AS ac_rcpt_description,
     ac_rcpt."payment_method" AS ac_rcpt_payment_method,
     ac_rcpt."receipt_no" AS ac_rcpt_receipt_no,
     ac_rcpt."receipt_type" AS ac_rcpt_receipt_type,
     ac_rcpt."received_date" AS ac_rcpt_received_date,
     ac_rcpt."reference_no" AS ac_rcpt_reference_no,
     ac_rcpt."remove_comment" AS ac_rcpt_remove_comment,
     ac_rcpt."source_no" AS ac_rcpt_source_no,
     ac_rcpt."total_amount" AS ac_rcpt_total_amount,
     ac_rcpt."total_applied" AS ac_rcpt_total_applied,
     ac_rcpt."total_received" AS ac_rcpt_total_received,
     ac_rcpt."total_payment" AS ac_rcpt_total_payment,
     ac_advc_pymt."c_ts" AS ac_advc_pymt_c_ts,
     extract (year from  ac_advc_pymt."c_ts") as OrderYear,
     to_char (  ac_advc_pymt."c_ts", 'Mon') as OrderMonth
FROM
     "public"."ac_actr" ac_actr INNER JOIN "public"."ac_stdn" ac_stdn ON ac_actr."id" = ac_stdn."id"
     INNER JOIN "public"."ac_chrt_code" ac_chrt_code ON ac_stdn."cohort_code_id" = ac_chrt_code."id"
     INNER JOIN "public"."ac_prgm_code" ac_prgm_code ON ac_chrt_code."program_code_id" = ac_prgm_code."id"
     INNER JOIN "public"."ac_prgm_level" ac_prgm_level ON ac_prgm_code."program_level_id" = ac_prgm_level."id"
     INNER JOIN "public"."ac_fcty_code" ac_fcty_code ON ac_prgm_code."faculty_code_id" = ac_fcty_code."id"
     INNER JOIN "public"."ac_acct" ac_acct ON ac_actr."id" = ac_acct."actor_id"
     INNER JOIN "public"."ac_advc_pymt" ac_advc_pymt ON ac_acct."id" = ac_advc_pymt."account_id"
     INNER JOIN "public"."ac_acdm_sesn" ac_acdm_sesn ON ac_advc_pymt."session_id" = ac_acdm_sesn."id"
      INNER JOIN "public"."ac_knof" ac_knof ON ac_advc_pymt."id" = ac_knof."advance_payment_id"
    INNER JOIN "public"."ac_rfnd_pymt" ac_rfnd_pymt ON ac_advc_pymt."id" = ac_rfnd_pymt."advance_payment_id"
     INNER JOIN "public"."ac_rcpt" ac_rcpt ON ac_advc_pymt."receipt_id" = ac_rcpt."id"
     LEFT OUTER JOIN "public"."ac_knof_item" ac_knof_item ON ac_knof."id" = ac_knof_item."knockoff_id"

WHERE  (to_char ( ac_advc_pymt."c_ts", 'YYYY-MM-DD' )>= $P{Start_date}
AND to_char ( ac_advc_pymt."c_ts", 'YYY-MM-DD') <= $P{End_date})
 AND ac_prgm_code."description" = $P{Program_code}
 AND  ac_prgm_level."code" = $P{Study_level}]]>
	</queryString>

AND to_char (  ac_knof."issued_date", 'YYY-MM-DD') <= $P{End_date})
 AND ac_prgm_code."description" = $P{Program_code}
 AND  ac_prgm_level."code" = $P{Study_level}
AND ac_knof."fd_st" = $P{Status}

ORDER BY
     ac_advc_pymt."reference_no" ASC]]>
